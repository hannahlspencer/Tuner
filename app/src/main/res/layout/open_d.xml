<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

    <Button
            android:text="Low D"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/opend_lowd"
            android:layout_marginTop="60dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="160dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.987"/>
    <Button
            android:text="A"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/opend_a"
            android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/opend_lowd" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="160dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.993"
    />
    <Button
            android:text="D"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/opend_d" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/opend_a" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="160dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.987"
    />
    <Button
            android:text="F#"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/opend_fsharp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.498"
            android:layout_marginTop="32dp" app:layout_constraintTop_toBottomOf="@+id/opend_d"/>
    <Button
            android:text="A"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/opend_higha" android:layout_marginTop="36dp"
            app:layout_constraintTop_toBottomOf="@+id/opend_fsharp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.498"
    />
    <Button
            android:text="D"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/opend_highd"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.498"
            android:layout_marginBottom="108dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/opend_higha"
            app:layout_constraintVertical_bias="0.414"/>

</androidx.constraintlayout.widget.ConstraintLayout>